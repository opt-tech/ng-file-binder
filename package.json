    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^2.4.2",
    "ngrx-store-freeze": "^0.2.1",
    "prettier": "1.10.2",
    "protractor": "~5.1.2",
    "rxjs": "^5.5.6",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "2.6.2",
    "viz": "^0.0.1",
    "zone.js": "^0.8.19"
