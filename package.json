  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "test:ci": "npm test -- -w=false -sr --progress=false",
    "lint": "./node_modules/.bin/nx lint && ng lint",
    "e2e": "ng e2e --app=sample",
